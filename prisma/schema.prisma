// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/matchdata.db"
}

model Matchdata {
  id Int @id @default(autoincrement())
  name String
  teamNumber String
  comp String
  matchNum Int
  matchType String
  teamScouted Int
  stationId String
  auto_show String
  auto_move String
  auto_lowHub Int
  auto_humanPlayer Int
  auto_upperHub Int
  teleop_groundPickup String
  teleop_terminalPickup String
  teleop_lowHub Int
  teleop_highHub Int
  end_climb String
  end_climbLevel String
  end_defense String
  end_notes String

  @@unique([teamScouted, stationId, matchNum, comp], name: "matchid")
}
